{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory name",
			"defaultValue": "ecladfdemo"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "[concat(parameters('factoryName'), '/HomeDataExcel')]",
			"type": "Microsoft.DataFactory/factories/datasets",
			"apiVersion": "2018-06-01",
			"properties": {
				"linkedServiceName": {
					"referenceName": "BlobStorage",
					"type": "LinkedServiceReference"
				},
				"parameters": {
					"filename": {
						"type": "string"
					}
				},
				"annotations": [],
				"type": "Excel",
				"typeProperties": {
					"location": {
						"type": "AzureBlobStorageLocation",
						"fileName": {
							"value": "@dataset().filename",
							"type": "Expression"
						},
						"container": "filelanding"
					},
					"sheetIndex": 0,
					"firstRowAsHeader": true
				},
				"schema": []
			},
			"dependsOn": []
		},
		{
			"name": "[concat(parameters('factoryName'), '/HomeDataSQLTable')]",
			"type": "Microsoft.DataFactory/factories/datasets",
			"apiVersion": "2018-06-01",
			"properties": {
				"linkedServiceName": {
					"referenceName": "AzureSQLDB",
					"type": "LinkedServiceReference"
				},
				"annotations": [],
				"type": "AzureSqlTable",
				"schema": [
					{
						"name": "CRIM",
						"type": "float",
						"precision": 15
					},
					{
						"name": "ZN",
						"type": "int",
						"precision": 10
					},
					{
						"name": "INDUS",
						"type": "float",
						"precision": 15
					},
					{
						"name": "CHAS",
						"type": "int",
						"precision": 10
					},
					{
						"name": "NOX",
						"type": "float",
						"precision": 15
					},
					{
						"name": "RM",
						"type": "float",
						"precision": 15
					},
					{
						"name": "AGE",
						"type": "float",
						"precision": 15
					},
					{
						"name": "DIS",
						"type": "float",
						"precision": 15
					},
					{
						"name": "RAD",
						"type": "int",
						"precision": 10
					},
					{
						"name": "TAX",
						"type": "int",
						"precision": 10
					},
					{
						"name": "PTRATIO",
						"type": "float",
						"precision": 15
					},
					{
						"name": "B",
						"type": "float",
						"precision": 15
					},
					{
						"name": "LSTAT",
						"type": "float",
						"precision": 15
					},
					{
						"name": "MEDV",
						"type": "float",
						"precision": 15
					}
				],
				"typeProperties": {
					"schema": "dbo",
					"table": "HomeData"
				}
			},
			"dependsOn": []
		},
		{
			"name": "[concat(parameters('factoryName'), '/CopyExcelPipeline')]",
			"type": "Microsoft.DataFactory/factories/pipelines",
			"apiVersion": "2018-06-01",
			"properties": {
				"activities": [
					{
						"name": "Copy Excel Data to Azure SQL",
						"type": "Copy",
						"dependsOn": [],
						"policy": {
							"timeout": "7.00:00:00",
							"retry": 0,
							"retryIntervalInSeconds": 30,
							"secureOutput": false,
							"secureInput": false
						},
						"userProperties": [],
						"typeProperties": {
							"source": {
								"type": "ExcelSource",
								"storeSettings": {
									"type": "AzureBlobStorageReadSettings",
									"recursive": true,
									"enablePartitionDiscovery": false
								}
							},
							"sink": {
								"type": "AzureSqlSink"
							},
							"enableStaging": false,
							"translator": {
								"type": "TabularTranslator",
								"typeConversion": true,
								"typeConversionSettings": {
									"allowDataTruncation": true,
									"treatBooleanAsNumber": false
								}
							}
						},
						"inputs": [
							{
								"referenceName": "HomeDataExcel",
								"type": "DatasetReference",
								"parameters": {
									"filename": {
										"value": "@pipeline().parameters.filename",
										"type": "Expression"
									}
								}
							}
						],
						"outputs": [
							{
								"referenceName": "HomeDataSQLTable",
								"type": "DatasetReference"
							}
						]
					}
				],
				"parameters": {
					"filename": {
						"type": "string",
						"defaultValue": "filename.xlsx"
					}
				},
				"annotations": []
			},
			"dependsOn": [
				"[concat(variables('factoryId'), '/datasets/HomeDataExcel')]",
				"[concat(variables('factoryId'), '/datasets/HomeDataSQLTable')]"
			]
		}
	]
}